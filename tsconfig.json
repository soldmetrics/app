{
  "extends": "expo/tsconfig.base",
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ],
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@assets/*": [
        "assets/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@config/*": [
        "src/config/*"
      ],
      "@context/*": [
        "src/context/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@theme/*": [
        "src/theme/*"
      ]
    }
  }
}
